{
  "name": "user-app",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "json-schema-to-typescript": "^15.0.4",
    "typescript": "^5.0.2"
  },
  "scripts": {
    "generate:types": "npx quicktype -s schema ui-schema.json -o ui-schema.d.ts",
    "watch:types": "npx quicktype -s schema ui-schema.json -o ui-schema.d.ts --watch",
    "build": "npm run generate:types && <your-existing-build-command>",
    "build:server": "tsc -p tsconfig.server.json"
  }
}